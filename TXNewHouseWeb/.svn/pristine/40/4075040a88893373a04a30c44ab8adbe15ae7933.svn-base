.comparepop .this-conx ul li a.this-del { position: absolute; right: 2px; top: 9px; height: 9px; width: 9px; overflow: hidden; display: block; background: url(../images/bg-comparepop-this-del-1.png) 0 0 no-repeat; }
.comparepop .this-conx ul li .this-del:hover { background: url(../images/bg-comparepop-this-del-2.png) 0 0 no-repeat; }
