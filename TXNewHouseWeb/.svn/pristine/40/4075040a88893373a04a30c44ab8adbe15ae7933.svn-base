.loubox ul li dl dt .close:hover, .big_img .close:hover { background: url(../images/colose_a.gif) left top no-repeat; }
.loubox ul li dl dt .big_pho { width: 26px; height: 19px; background: url(../images/big_icon.png) left top no-repeat; position: absolute; bottom: 0; _bottom: 2px; right: 0; display: inline-block; }
.loubox ul li dl dt .big_pho:hover { background: url(../images/big_icon.png) right top no-repeat; }
.loubox ul li dl dt .shetxt { position: absolute; bottom: 0; left: 1px; font-size: 12px; height: 16px; display: inline-block; color: #0068b7; }
