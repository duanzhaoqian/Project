using System;

namespace TXModel.Dev
{
    /// <summary>
    /// 登录用户信息
    /// </summary>
    [Serializable]
    public class CT_LoginUser
    {
        //默认构造
        public CT_LoginUser()
        {
            Id = 0;
            LoginName = String.Empty;
            Pwd = String.Empty;
            Timestamp = 0;
            IsLogin = false;
        }
        /// <summary>
        /// 用户ID
        /// </summary>
        public int Id { get; set; }
        /// <summary>
        /// 登录名
        /// </summary>
        public string LoginName { get; set; }
        /// <summary>
        /// 密码（未使用）
        /// </summary>
        public string Pwd { get; set; }
        /// <summary>
        /// 时间戳
        /// </summary>
        public long Timestamp { get; set; }
        /// <summary>
        /// 是否已登录
        /// </summary>
        public bool IsLogin { get; set; }
    }
}
