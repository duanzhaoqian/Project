using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace TXCommons.PictureModular
{
    public class DocumentInfo
    {
        public int ID
        {
            get;
            set;
        }

        public string FileName
        {
            get;
            set;
        }

        public string Path
        {
            get;
            set;
        }

        public string DocumentType
        {
            get;
            set;
        }
    }

    /// <summary>
