.ac_results {
	padding: 0px;
	border: 1px solid #C5C5C5;
	background-color: white;
	overflow: hidden;
	z-index: 99999;
	font-size:12px;
}
.ac_results .tips {
    position: relative;
    height: 30px;
    line-height: 30px;
    background: #F7F7F7;
    padding: 0 10px;
    font-size: 12px;
    color: #434950;
}
.ac_results ul {
	width: 100%;
	list-style-position: outside;
	list-style: none;
	padding: 0;
	margin: 0;
}

.ac_results li {
	margin: 0px;
	padding: 6px 5px;
	cursor: default;
	display: block;
	/* 
	if width will be 100% horizontal scrollbar will apear 
	when scroll mode will be used
	*/
	/*width: 100%;*/
	font: menu;
	font-size: 12px;
	/* 
	it is very important, if line-height not setted or setted 
	in relative units scroll will be broken in firefox
	*/
	line-height: 16px;
	white-space: nowrap;
    list-style-type: none;
	overflow: hidden;
}

.ac_loading {
	/*background: white url('indicator.gif') right center no-repeat;*/
}

.ac_odd {
	background-color: #fff;
}

.ac_over {
	background-color:#FFE3F2;
	color: #000;
}
.ac_inputvalue{color:red;}
.ac_name{float:left;}
.ac_num{float:right;color:green;}