@charset "utf-8";
/*--------- 重置 by nxd 2012.10.30 */
/*--------- reset */
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, input, textarea, p, blockquote, th, td {
	margin:0;
	padding:0
}
table {
	border-collapse: collapse;
	border-spacing: 0
}
img {
	border: 0
}
address, caption, cite, code, dfn, em, strong, th, var {
	font-style: normal;
	font-weight: normal
}
ol, ul {
	list-style: none
}
caption, th {
	text-align: left
}
h1, h2, h3, h4, h5, h6 {
	font-size: 100%;
	font-weight: normal
}
body {
	height:100%;
}
select {
	font-size:12px
}
button {
	line-height: 1.231;
	white-space: nowrap;
	vertical-align: middle;
	cursor: pointer
}.btnP{cursor: pointer}
strong {
	font-weight: 800
}
i{ font-style: normal}
