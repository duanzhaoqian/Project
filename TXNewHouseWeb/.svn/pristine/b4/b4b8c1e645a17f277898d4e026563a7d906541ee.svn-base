	.condGroup span.hover{
		position:relative;
		display: inline;float:left;height: 20px;line-height: 20px;margin: 3px 6px 3px 0;
		padding: 0 0 0 6px;white-space: nowrap;}
	.condGroup span{border: 1px solid #e4e4e4; background:#f4f4f4;}
