		.condGroup span.hover .delete {background:url(../images/icon_delete1.gif) 0 4px no-repeat;}
	.condGroup .delete{
		margin:0 5px; cursor:pointer; display:inline; height:20px; text-decoration:none;
		background:url(../images/icon_delete0.gif) 0 4px no-repeat;}
