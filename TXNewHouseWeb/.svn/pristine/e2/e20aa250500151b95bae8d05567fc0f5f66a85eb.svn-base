/* 楼盘对比 */
.comparepop { width: 190px; border: 1px solid #70a711; overflow: hidden; position: fixed; right: 25px; z-index: 99; top: 137px; }
.comparepop .this-title { position: relative; background: #70a711; height: 31px; line-height: 31px; color: #FFF; font-weight: bold; font-size: 14px; padding-left: 10px; }
.comparepop .this-title a.shouqi { background: url(../images/a-shouqi.gif) 0 0 no-repeat; width: 32px; height: 31px; display: block; position: absolute; right: 0; top: 0; text-indent: -1111px; }
.comparepop .this-conx { background: #f7fbf6; padding: 5px 10px; }
.comparepop .this-conx ul li { position: relative; border-bottom: 1px #bfd7b9 dashed; height: 28px; line-height: 28px; }
.comparepop .this-conx ul li .this-repair { vertical-align: 2px; }
.comparepop .this-conx ul li a.this-del { position: absolute; right: 2px; top: 9px; height: 9px; width: 9px; overflow: hidden; display: block; background: url(../images/bg-comparepop-this-del-1.png) 0 0 no-repeat; }
.comparepop .this-conx ul li .this-del:hover { background: url(../images/bg-comparepop-this-del-2.png) 0 0 no-repeat; }
a.comparepop-btn { background: url(../images/bg-comparepop-btn.png) 0 0 no-repeat; width: 24px; height: 104px; position: fixed; right: 0px; z-index: 99; top: 137px; }
