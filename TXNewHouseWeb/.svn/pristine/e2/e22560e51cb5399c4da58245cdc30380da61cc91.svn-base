@charset "utf-8";
/* CSS Document */
/* manage by nxd 2012.11.04 */
.wp_manage { width:100%; text-align:left; background:#f5f5f5; font-family:Arial, Helvetica, sans-serif, "宋体";}
	.wp_manage a { color:#0066cc}
	.wp_manage a:hover {color:#c60000}
	.wp_manage .banner{ background:#487300; height:81px; position:relative; z-index:888;}
		.wp_manage .banner .logo { position:absolute; left:15px; top:10px;}
		.wp_manage .banner .nav  { float:left; margin:51px 0 0 190px; _margin:51px 0 0 100px}
		.wp_manage .banner .nav a{ width:120px; height:30px; line-height:30px;
		background:#63950e; color:#FFF; font-size:14px; text-align:center;
		display:block; float:left; margin-right:10px}
		.wp_manage .banner .nav a:hover{ 
		text-decoration:none; background:url(../images/bg_navMenuHover.gif) 0 0 repeat-x;}
		.wp_manage .banner .nav a.current { background:#f5f5f5; color:#333333; font-weight:bold}
	.wp_manage .banner .operation { 
		position:absolute; right:5px; top:15px; color:#b2db37; font-size:12px}
		.wp_manage .banner .operation a{color:#c8e19a;}
		.wp_manage .banner .operation span.green{ color:#71aa10}
	/*内容包裹器*/
	.wp_manage .conBox { padding:5px 15px; min-height:150px; overflow:visible; background:#f5f5f5;}		
	.wp_manage .conBox .sidebar{ width:160px; float:left;}		
	.wp_manage .conBox .main {  margin-left:160px; padding-top:10px}		
	.wp_manage .footer { 
		background:url(../images/bg_footerBorder.gif) #f5f5f5 left top repeat-x;
		margin-top:15px; padding:18px; color:#999; text-align:center   }
		.wp_manage .footer p{ line-height:25px; font-size:12px}
	/*面包屑*/
	.wp_manage .conBox .main .current{
		border:1px solid #dddddd; height:27px; font-size:12px;
		background:url(../images/bg_current.gif) left center repeat-x }
		.wp_manage .conBox .main .current i { padding:0 5px; font-size:14px; color:#999}
	.wp_manage .conBox .main .current .root{
		background:#f1f1f1 url(../images/bg_current_root.gif) right center no-repeat;
		padding-right:25px; display:inline-block; height:27px; line-height:27px;
		text-align:center; text-indent:25px;
		*display:inline; *padding-top:8px; *padding-bottom:5px;*padding-left:25px;
		*text-indent:0 }
	.wp_manage .conBox .main .current a { color:#333}
	.wp_manage .conBox .main .current a:hover {color:#c60000}	
	.wp_manage .conBox .main .data { border:1px solid #dddddd;  margin:0 auto; margin-top:10px; background:#fff}
	/*筛选*/
	.wp_manage .conBox .main .data .filterBar { 
		background:#ecedea; padding:15px; position:relative;}
	.wp_manage .conBox .main .data .filterBar .dingwei { 
		position:absolute; right:22px; top:15px;}
	/*表格*/
	.DataTableA { color:#333; font-size:12px;}
		.DataTableA th { height:35px; border:1px solid #ddd; border-left:0;
		background:url(../images/tab_head.gif) 0 0 repeat-x; text-align:left; padding:0 10px;}
		.DataTableA td { background:#FFF; border-bottom:1px solid #ddd; padding:8px 10px;
		text-align:left; color:#666}
		.DataTableA th.lasted,
		.DataTableA td.lasted{ border-right:none 0}
		.DataTableA .eqc{ background:#fff9ee}
		.DataTableA .eqc td { background:#fff9ee}
	.wp_manage .conBox .main .data .paging { 
	padding:15px 25px; line-height:35px \9; position:relative; text-align:right; font-size:12px}
	/*手风琴*/
	.accordion_box {
		border:1px solid #dddddd; border-bottom:none; border-top:none;
		position:relative; margin-top:35px; width:149px
	}
	.accordion_box h5{
		top:-26px; position:absolute;
		background:url(../images/BG_accordion_box.gif) center top no-repeat;
		width:150px; height:27px; overflow:hidden; text-indent:-1000px}
	.accordion {
		border-top: 1px solid #ffffff;
		border-bottom: 1px solid #e5e5e5;
		background:#f6f6f6;
	}
	.accordion-header,
	.accordion-header-active{
		height:30px; line-height:30px; font-weight:bold;
		color:#333333; font-size:12px;	padding-left:18px;
		background:url(../images/BG_accordion_box1.gif) no-repeat; cursor:pointer}
	.accordion-header {
		}
	.accordion-header-active {
		background:url(../images/BG_accordion_box1-2.gif) no-repeat;
		border-bottom:1px solid #ddd}
	.accordion-content {
		font-weight:bold; text-indent:25px
	}
	.accordion-content a{ 
		display:block; background:#ffffff; color:#333; position:relative;
		font-size:12px; padding:8px 0; font-weight:normal; text-decoration:none}
	.accordion-content a:hover { background:#f5f5f5; text-decoration:none; color:#487300}
	.accordion-content a.current {background:#89a758; color:#FFF; font-weight:bold;}
	
	.accordion-content a span { 
		position:absolute; right:10px; top:6px; font-weight:normal;
		font-size:14px; font-family:Arial; color:#FFF}
		.accordion-content a:hover span { color:#89a758}
		
