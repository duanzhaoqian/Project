@charset "utf-8";
/* CSS Document */
/*ȫʽ*/
html{color:#656565;}
body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0;}
/**/

.w-390{ width:350px; height :330px;}


.borderg{border:0px solid #669910; }
.meBan-box {float: left;background-color:#fff; border: 3px solid #315888;}
.meBan-box .hd b{ float:left; font-size:14px;}
.meBan-box .hd a {background: url(images/zm-delet.gif) no-repeat left center; float: right; height: 12px;width: 12px; top:10px; right:10px; position:relative;}
.meBan-box .hd{ border-bottom:1px solid #CCCCCC; background-color:#F7F7F7;  height:31px; line-height:31px; padding-left:12px; }
.meBan-box .content{margin:0; padding:0;}
.meBan-box .bd{ line-height:25px;background:#fff;margin:0;}

.meBan-box .top{ height:91px; background:url(images/pop_bd.jpg) repeat-x;}


.meBan-box .bottom{ height:71px; background:url(images/bg4.gif) repeat-x; border-top:1px solid #E4E4E4}
.meBan-box .bottom p{ background-position:16px -305px; padding-left:35px}

/*dialog*/
.dialog-overlay {background: gray; filter: alpha(opacity:50); opacity: 0.5}
.dialog {padding-right: 10px; padding-left: 10px; background: gray; filter: alpha(opacity:70); padding-bottom: 10px; padding-top: 10px; opacity: 0.9; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px}
.dialog .hd {	padding-right: 6px; padding-left: 6px; min-height: 15px; background: #000; padding-bottom: 6px; cursor: move; color: #fff; padding-top: 6px}
.meBan-box .hd .bidBar {float: left; margin-right: 10px; color:#315888;}
.meBan-box .hd .close {float: right; cursor: pointer; text-decoration: underline}
.dialog .content {padding-right: 10px; padding-left: 10px; background: #fff; padding-bottom: 10px; padding-top: 10px}
.dialog iframe {width: 100%; height: 100%}
.content img {max-width: 700px; overflow: auto; max-height: 500px}
