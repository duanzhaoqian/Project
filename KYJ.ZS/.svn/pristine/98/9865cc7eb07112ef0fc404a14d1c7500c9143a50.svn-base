using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace KYJ.ZS.Models.DB
{
    /// <summary>
    /// 收藏商品
    /// </summary>
    public class CollectionGoods
    {
        /// <summary>
        /// 主键ID
        /// </summary>
        public int Id { get; set; }
        /// <summary>
        /// 商品ID
        /// </summary>
        public int GoodsId { get; set; }
        /// <summary>
        /// 商品类型（0 未知，1 租，2 售）
        /// </summary>
        public int Type { get; set; }
        /// <summary>
        /// 收藏标题
        /// </summary>
        public string Title { get; set; }
        /// <summary>
        /// 商品GUID
        /// </summary>
        public string Guid { get; set; }
        /// <summary>
        /// 商品月租金
        /// </summary>
        public decimal MonthPrice { get; set; }
        /// <summary>
        /// 收藏URL
        /// </summary>
        public string Url { get; set; }
        /// <summary>
        /// 是否售完
        /// </summary>
        public bool IsSellOut { get; set; }
        /// <summary>
        /// 是否删除
        /// </summary>
        public bool IsDel { get; set; }
    }
}
