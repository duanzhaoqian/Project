using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace KYJ.ZS.Models.Carts
{
    /// <summary>
    /// Author：邓福伟
    /// Time: 204-4-17
    /// Desc: 购物车信息实体类
    /// </summary>
    public class CartEntity
    {
        /// <summary>
        ///  购物车的唯一标识GUID
        /// </summary>
        public string CartGuid { get; set; }
        /// <summary>
        /// 普通用户Id
        /// </summary>
        public int UserId { get; set; }
        /// <summary>
        /// 商品用户Id（用户/商品Id）
        /// </summary>
        public int OperId { get; set; }
        /// <summary>
        /// 商品Id
        /// </summary>
        public int GoodsId { get; set; }
        /// <summary>
        /// 颜色Id
        /// </summary>
        public int? ColorId { get; set; }
        /// <summary>
        /// 颜色名称
        /// </summary>
        public string ColorName { get; set; }
        /// <summary>
        /// 规格Id
        /// </summary>
        public int? NormId { get; set; }
        /// <summary>
        /// 规格名称
        /// </summary>
        public string NormName { get; set; }
        /// <summary>
        /// 商品租期
        /// </summary>
        public int? GoodsLease { get; set; }
        /// <summary>
        /// 商品数量
        /// </summary>
        public int GoodsNum { get; set; }
        /// <summary>
        /// 商品价格（商品单价/商品月租）
        /// </summary>
        public double GoodsPrice { get; set; }
        /// <summary>
        /// 商品类型：0 未知，1 租，2 售
        /// </summary>
        public int GoodsType { get; set; }
        /// <summary>
        /// 修改时间
        /// </summary>
        public DateTime? UpdateTime { get; set; }
        /// <summary>
        /// 创建时间
        /// </summary>
        public DateTime? CreateTime { get; set; }
    }
}
